  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "format": "npx prettier --write .",
    "lint": "npx prettier --check .",
    "lint:fix": "eslint './**/*.{js,jsx,ts,tsx}' --fix",
