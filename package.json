    "@react-native/metro-config": "^0.76.1",
    "expo": "~52.0.28",
    "expo-blur": "^14.0.3",
    "expo-font": "~13.0.3",
    "expo-linear-gradient": "~14.0.2",
    "expo-localization": "~16.0.1",
    "expo-router": "^4.0.17",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.7",
    "i18next": "^23.16.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-i18next": "^15.1.0",
